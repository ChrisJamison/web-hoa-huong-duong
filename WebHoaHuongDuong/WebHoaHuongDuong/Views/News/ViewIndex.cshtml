@model IEnumerable<DataModel.News>
<div class="benphai float_l">
    <div style="display: none">
        <img src="http://bncvn.net/index.php?mod=imgqc&domain=mapu.vn" /></div>
    <div class="module relative">
        <div class="module_title">
            <ul class="unlnavi" style="width: 94%">
                <li id="tab_link-01_01" class="acti page_small"><span class="icon_ttuc"></span>
                    <h1>
                        <a href="javascript:void()">Thực phẩm</a></h1>
                </li>
            </ul>
        </div>
        <div class="clear"></div>
        <div class="module_body">
            <span class="vach_top"></span>

            <div class="clear relative tab_show">
                <div class="showop float_r">
                    
                    <div class="clear"></div>
                </div>
                <div class="tab_ul float_l">
                    <ul id="tab-05" class="ulnavi2">
                        <li id="tab_link-01_01" class="acti2"><a href="javascript:void();">Mới</a></li>
                        <li id="tab_link-02_01"><a href="javascript:void();">Nổi bật</a></li>
                        <li id="tab_link-03_01"><a href="javascript:void();">A - Z</a></li>
                    </ul>
                </div>
                <!--<div class="border_bottom2"></div>-->
            </div>
            <div class="newst clear">
                <div class="content _tab-05 tab_link-01_01">
                    @foreach (var item in Model)
                    {
                        <div class="khung_tip">
                            <h3 class="title_news_n"><a href="~/News/ViewDetail/@item.News_ID" title="Nước chanh có thực sự giúp giảm cân?">@item.Title</a>
                            </h3>
                            <span class="time_news" style="color: #666">@item.DateUpdate</span>
                            <div class="clear"></div>
                            <div class="img_news">
                                <a class="tip _img" href="~/News/ViewDetail/@item.News_ID">
                                    <img alt="Nước chanh có thực sự giúp giảm cân?" src='~/Images/News/@item.Thumbnail' onerror="this.onerror = null;this.src = 'https://upload.webbnc.net/view.php?image=upload/web/51/513168/news/2015/07/19/06/06/143730401880.jpg&mode=crop&size=500x370'" />
                                </a>
                            </div>
                            <div class="textinner clear">Chi tiết...</div>
                         
                            <div class="clear"></div>
                        </div>
                    }
                    <div class="clear"></div>
                </div>
                <div class="content _tab-05 tab_link-02_01 hide">
                     @foreach (var item in Model)
                    {
                        <div class="khung_tip">
                            <h3 class="title_news_n"><a href="~/News/ViewDetail/@item.News_ID" title="Nước chanh có thực sự giúp giảm cân?">@item.Title</a>
                            </h3>
                            <span class="time_news" style="color: #666">@item.DateUpdate</span>
                            <div class="clear"></div>
                            <div class="img_news">
                                <a class="tip _img" href="~/News/ViewDetail/@item.News_ID">
                                    <img alt="Nước chanh có thực sự giúp giảm cân?" src='~/Images/News/@item.Thumbnail' onerror="this.onerror = null;this.src = 'https://upload.webbnc.net/view.php?image=upload/web/51/513168/news/2015/07/19/06/06/143730401880.jpg&mode=crop&size=500x370'" />
                                </a>
                            </div>
                            <div class="textinner clear">Chi tiết...</div>
                         
                            <div class="clear"></div>
                        </div>
                    }
                    <div class="clear"></div>
                </div>
                <div class="content _tab-05 tab_link-03_01 hide">
                   @foreach (var item in Model)
                    {
                        <div class="khung_tip">
                            <h3 class="title_news_n"><a href="~/News/ViewDetail/@item.News_ID" title="Nước chanh có thực sự giúp giảm cân?">@item.Title</a>
                            </h3>
                            <span class="time_news" style="color: #666">@item.DateUpdate</span>
                            <div class="clear"></div>
                            <div class="img_news">
                                <a class="tip _img" href="~/News/ViewDetail/@item.News_ID">
                                    <img alt="Nước chanh có thực sự giúp giảm cân?" src='~/Images/News/@item.Thumbnail' onerror="this.onerror = null;this.src = 'https://upload.webbnc.net/view.php?image=upload/web/51/513168/news/2015/07/19/06/06/143730401880.jpg&mode=crop&size=500x370'" />
                                </a>
                            </div>
                            <div class="textinner clear">Chi tiết...</div>
                         
                            <div class="clear"></div>
                        </div>
                    }
                    <div class="clear"></div>
                </div>
                <div class="clear"></div>

            </div>
            <div class="clear"></div>
        </div>
        <span class="vach_bot2"></span>
    </div>
</div>



