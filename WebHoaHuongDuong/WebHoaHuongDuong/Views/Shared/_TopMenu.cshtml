<!-- Skitter-->
<div style="max-width: 1170px; position: relative; margin: 0 auto;">
    <div class="clear"></div>
    <div class="slideshow">
        <div class="box_skitter box_skitter_large">
            <ul>
                <li>
                    <a href="~/Home" title="" target="_blank">
                        <img width="1170" height="450" title="" src='~/imgs/qc/1.jpg' alt='images/143105800154.png' />
